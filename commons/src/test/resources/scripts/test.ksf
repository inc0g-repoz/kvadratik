// Commenting
on test(e) {
	var d = e.call(2) ^ 4;
	e.logger.info(d);

	var bool = (false && 1 == 2) || false || (1 + 2 == 3);
	e.logger.info(bool);

	var i = 0;
	while (i < 2) {
		i = i + 1;
		e.logger.info(i);
	}

	for (var j = 1; j <= 2; j = j + 1) {
		e.logger.info(j);
	}

	if (!true) e.logger.info("true");
	else e.logger.info("false");

	var arr = e.arr[0];
	e.logger.info(arr);
}
